{
  "name": "COBOL",
  "scopeName": "source.cobol",
  "fileTypes": ["cbl", "cob", "cobol", "cpy"],
  "patterns": [
    {
      "name": "comment.line.cobol",
      "match": "^\\s{0,7}[\\*].*$",
      "comment": "COBOL comment line (position 7)"
    },
    {
      "name": "comment.line.cobol",
      "match": "^      \\*>.*$",
      "comment": "COBOL inline comment"
    },
    {
      "name": "string.quoted.double.cobol",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.cobol",
          "match": "\"\""
        }
      ]
    },
    {
      "name": "string.quoted.single.cobol",
      "begin": "'",
      "end": "'",
      "patterns": [
        {
          "name": "constant.character.escape.cobol",
          "match": "''"
        }
      ]
    },
    {
      "name": "constant.numeric.cobol",
      "match": "\\b([0-9]+\\.?[0-9]*|\\.[0-9]+)([eE][+-]?[0-9]+)?\\b"
    },
    {
      "name": "keyword.control.division.cobol",
      "match": "\\b(IDENTIFICATION|ENVIRONMENT|DATA|PROCEDURE)\\s+(DIVISION)\\b",
      "captures": {
        "1": {
          "name": "keyword.control.cobol"
        },
        "2": {
          "name": "keyword.control.cobol"
        }
      }
    },
    {
      "name": "keyword.control.section.cobol",
      "match": "\\b(CONFIGURATION|INPUT-OUTPUT|FILE|WORKING-STORAGE|LINKAGE|FILE-CONTROL|I-O-CONTROL)\\s+(SECTION)\\b",
      "captures": {
        "1": {
          "name": "keyword.control.cobol"
        },
        "2": {
          "name": "keyword.control.cobol"
        }
      }
    },
    {
      "name": "keyword.control.cobol",
      "match": "\\b(ACCEPT|ACCESS|ADD|AFTER|ALL|ALLOCATE|ALTER|ALTERNATE|AND|ANY|AREA|AREAS|ARE|ARGUMENT|ARGUMENTS|ARISE|AS|ASCENDING|ASSIGN|AT|ATTRIBUTE|AUTHOR|AUTO|AUTOMATIC|BEFORE|BINARY|BLANK|BLOCK|BOTTOM|BY|CALL|CANCEL|CHAIN|CHARACTER|CHARACTERS|CLASS|CLASS-ID|CLOSE|CODE|COLLATING|COLUMN|COMMA|COMMON|COMMUNICATION|COMP|COMPUTE|CONFIGURATION|CONNECT|CONNECTOR|CONTAINS|CONTENT|CONTINUE|CONTROL|CONTROLS|CONVERT|CORRESPONDING|COUNT|CURRENCY|CURSOR|CYCLE|DATE|DATE-COMPILED|DATE-WRITTEN|DAY|DAY-OF-WEEK|DBCS|DE|DEBUG|DEBUGGING|DECIMAL-POINT|DECLARATIVES|DEFAULT|DELETE|DELIMITED|DELIMITER|DEPENDING|DESCENDING|DESTINATION|DETAIL|DIVIDE|DIVISION|DOWN|DUPLICATE|DUPLICATES|DYNAMIC|EBCDIC|ECHO|ENTRY|ENVIRONMENT|ERROR|ESCAPE|EVALUATE|EXCEPTION|EXHIBIT|EXIT|EXTEND|EXTERNAL|FD|FILE|FILE-CONTROL|FILLER|FINAL|FIRST|FOOTING|FOR|FORCE|FROM|FUNCTION|GENERATE|GIVING|GLOBAL|GOBACK|GREATER|GRID|GROUP|HEADING|HIGH-VALUE|HIGH-VALUES|HOLD|I-O-CONTROL|ID|IDENTIFICATION|IF|IN|INDEX|INDEXED|INDICATES|INITIAL|INITIALIZE|INITIATE|INPUT|INPUT-OUTPUT|INSPECT|INSTALL|INTO|INVALID|IS|JUST|KEY|KEYBOARD|LABEL|LAST|LEADING|LENGTH|LESS|LIMIT|LIMITS|LINAGE|LINAGE-COUNTER|LINE|LINE-COUNTER|LINES|LINKAGE|LIST|LITERAL|LOCAL-STORAGE|LOCK|LOW-VALUE|LOW-VALUES|LOWER-BOUND|LOWER-BOUNDS|MAP|MASTER|MAXOCCURS|MEMORY|MERGE|MESSAGE|METACLASS|METHOD|MIDDLE|MINUS|MINOCCURS|MODE|MODIFIERS|MODULARITY|MOVE|MULTIPLE|MULTIPLY|NATIVE|NCHARACTER|NATIVE|NEAR|NEGATIVE|NESTED|NEXT|NO|NOASSIGN|NOBACKING|NOEMPTY|NOINVOKE|NOLABEL|NOMINAL|NOT|NOTE|NOTYPE|NUMBER|NUMERIC|OBJECT|OCCURS|OF|OFF|OMITTED|ON|ONLY|OPEN|OPTIONAL|OR|ORDER|ORGANIZATION|OTHER|OUTPUT|OUTSIDE|OVERFLOW|OVERRIDE|PACKED-DECIMAL|PADDING|PAGE|PAGE-COUNTER|PARAGRAPH|PARSE|PASSWORD|PERFORM|PH|PHYSICAL|PICTURE|PLUS|POINTER|POSITION|POSITIVE|PRECEDENCE|PRECISION|PERFORM|PROCEDURE|PROCEED|PROCESS|PRODUCING|PROGRAM|PROGRAM-ID|PROMPT|PROPERTY|PROTOTYPE|PURGE|QUEUE|RAISING|RANDOM|RD|READ|RECEIVE|RECORD|RECORDS|RECURSIVE|REDEFINES|REEL|REFERENCE|REFERENCES|RELATIVE|RELEASE|REMAINDER|REMARKS|REMOTE|REMOVE|RENAMES|REPLACING|REPORT|REPORTING|REPORTS|REPOSITORY|REQUIRED|REQUIRES|REREAD|RERUN|RESET|RESIGNAL|RESOURCE|RESULT|RESULT-TYPE|RESUME|RETAIN|RETIRED|RETURN|RETURNING|REVERSE-VIDEO|REVERSED|REWIND|RIGHT|RUN|SAME|SCALE|SCREEN|SCROLL|SD|SEARCH|SECTION|SECURITY|SEGMENT|SEGMENT-LIMIT|SELECT|SELECTION|SELECTIVE|SELF|SEND|SENTENCE|SEPARATE|SEQUENCE|SEQUENTIAL|SET|SHARING|SIGNAL|SIZE|SORT|SORT-MERGE|SOURCE|SOURCE-COMPUTER|SPACE|SPACES|SPECIAL-NAMES|STANDARD|STANDARD-1|STANDARD-2|START|STATE|STATEMENT|STATIC|STATUS|STEP|STOP|STRATEGY|STRING|STRUCTURE|STYLE|SUB-QUEUE-1|SUB-QUEUE-2|SUB-QUEUE-3|SUBPROGRAM|SUBTRACT|SUFFIX|SUM|SUPER|SUPPRESS|SYMBOL|SYMBOLIC|SYNC|SYNCHRONIZED|SYSTEM|TABLE|TALLYING|TAPE|TASK|TERMINATE|TEST|TEXT|THAN|THEN|THEN|THREAD|THROUGH|THRU|TIES|TIGHT|TIME|TIMEOUT|TIMES|TITLE|TO|TOP|TRADITIONAL|TRANSFORM|TRANSITION|TRUNCATE|TYPE|TYPES|UNDERLINE|UNIT|UNSTRING|UNTIL|UPPER|UPPER-BOUND|UPPER-BOUNDS|UPON|UPDATE|USAGE|USE|USER|USER-DEFINED|USING|VALIDATE|VALIDATION|VALUE|VALUES|VARYING|VCELL|VPADDING|VPOSITION|VSCROLL|VT|VTERMINAL|VTEXT|VTOP|VTRIM|WBACKGROUND|WBORDER|WBOTTOM|WCLOSE|WCENTER|WCONTROL|WCURSOR|WDESCRIPTION|WDISABLE|WDISPLAY|WENABLE|WFOCUS|WFONT|WFOREGROUND|WFRAMECOLOR|WFRAMESIZE|WFRAMETITLE|WHANDLE|WHEIGHT|WHIDE|WHORIZONTAL|WIMAGE|WINPUT|WINSERT|WINVERTIMAGE|WLABEL|WLEFT|WMENU|WMENUBAR|WMENUITEM|WMODAL|WMOUSEEVENT|WMOUSEMASK|WMOVEWINDOW|WMULTILINE|WPARENT|WPOPUP|WQUESTION|WRADIOBUTTON|WREADONLY|WREFRESH|WRESIZABLE|WRIGHT|WSCROLL|WSCROLLBAR|WSCROLLSTYLE|WSECURE|WSELECTABLE|WSELECT|WSHADE|WSHAKING|WSHAPE|WSIZE|WSTATUSBAR|WSTYLE|WSUNKEN|WTABGROUP|WTABSTOP|WTEXTCOLOR|WTITLE|WTITLEBAR|WTITLEBUTTON|WTOP|WTRACE|WTRANSPARENT|WTRIGGER|WTRIM|WTRIMCOLOR|WUNFRAMED|WUNSCROLLABLE|WVARIANT|WVERTICAL|WVISIBLE|WWIDTH|WWINDOW|WWINDOWMENU|WITH|WORKING-STORAGE|WRITE|XMLSCHEMA|YEAR|YES)\\b"
    },
    {
      "name": "keyword.operator.cobol",
      "match": "(\\*\\*|\\+|-|/|=|<|>|<=|>=|<>|\\(|\\))"
    },
    {
      "name": "entity.name.function.cobol",
      "match": "\\b(FUNCTION|ACCEPT|CALL|PERFORM|OPEN|CLOSE|READ|WRITE|DELETE)\\s+",
      "captures": {
        "1": {
          "name": "entity.name.function.cobol"
        }
      }
    },
    {
      "name": "punctuation.terminator.cobol",
      "match": "\\."
    },
    {
      "name": "variable.other.cobol",
      "match": "\\b[A-Z][A-Z0-9\\-]*\\b"
    }
  ],
  "repository": {}
}
